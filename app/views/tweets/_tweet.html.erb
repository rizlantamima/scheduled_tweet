<div id="<%= dom_id tweet %>">
  <p>
    <strong>User:</strong>
    <%= tweet.user_id %>
  </p>

  <p>
    <strong>Twitter account:</strong>
    <%= tweet.twitter_account_id %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= tweet.body %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= tweet.published_at %>
  </p>

  <p>
    <strong>Tweet:</strong>
    <%= tweet.tweet_id %>
  </p>

</div>
